<!DOCTYPE HTML>
<html>
  <head>
    <title><%= render :partial => 'layouts/umn_bootstrap/title' %></title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <%= stylesheet_link_tag    "application", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <div class="container-fluid">

      <%= render :partial => 'layouts/umn_bootstrap/common/header' %>

      <div class="jumbotron">
        <%= render :partial => 'layouts/umn_bootstrap/application_header' %>
      </div>

      <div class="row-fluid">
        <div class="span3">
          <%= render :partial => 'layouts/umn_bootstrap/navigation' %>
        </div>
        <div class="span9">
          <%= yield %>
        </div>
      </div>

      <%= render :partial => 'layouts/umn_bootstrap/common/footer' %>

    </div>
  </body>
</html>


