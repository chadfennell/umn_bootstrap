<!DOCTYPE html>
<html lang="en">
  <head>
	<%= render :partial => 'layouts/umn_bootstrap/common/head' %>
  </head>
  <body>
 	<%= render :partial => 'layouts/umn_bootstrap/common/header' %>
    <div class="navbar navbar-fluid-top">
      <div class="navbar-inner">
        <div class="container-fluid">
          <%=link_to  root_path, :class => "brand" do%>
          <%= render :partial => 'shared/umn_bootstrap/application_header' %> 
          <%end%>
          <a class="btn btn-navbar" data-target=".nav-collapse" data-toggle="collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </a>
          <div class="container-fluid nav-collapse">
            <%= render :partial => 'shared/umn_bootstrap/navigation' %>
          </div><!--/.nav-collapse -->
        </div>
      </div>
    </div>

    <div class="container-fluid">
      <div class="row-fluid">
 
        <div class="span12">
          <%= bootstrap_flash %>
          <%= yield %>
          <hr />
        </div>
     
      </div><!--/row-->
      
	<%= render :partial => 'layouts/umn_bootstrap/common/footer' %>
	<%= render_if_exists :partial => 'shared/umn_bootstrap/analytics' %>
    </div> <!-- /container -->

  </body>
</html>
